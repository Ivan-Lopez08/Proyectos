public class Main {
    public static void main(String[] args) {
        int resultado = suma(10,20,30);
        System.out.println("Suma de los valores: "+resultado);

        coche toyota = new coche();
        toyota.masPuertas();
        System.out.println("Numero de puertas: "+toyota.puertas);
    }
    public static int suma(int a, int b, int c){
        return (a+b+c);
    }
}

class coche{
    public int puertas= 4;
    public void masPuertas(){
        this.puertas ++;
    }
}
