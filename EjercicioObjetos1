class Alumno:
    nombre = None
    nota = None

    def __init__(self, nombre, nota):
        self.nombre = nombre
        self.nota = nota

    def imprimir (self):
        print("Nombre: ",self.nombre)
        print("Nota: ",self.nota)

    def aprobado (self):
        if self.nota >= 60:
            print("El alumno ",self.nombre," ha aprobado con ",self.nota)
        else:
            print("El alumno ",self.nombre," no ha aprobado, se queda con ",self.nota)

a = Alumno("Jose", 87.2)

a.imprimir()
a.aprobado()
